{
	"main": "index.js",
	"name": "deerx",
	"private": true,
	"scripts": {
		"install": "cd ios && (bundle install & pod install --repo-update)",
		"dev:ios": "./bin/setup_dev.sh && react-native run-ios --scheme app --simulator \"iPhone 12\"",
		"stage:ios": "./bin/setup_stage.sh && react-native run-ios --scheme app --simulator \"iPhone 12\"",
		"prod:ios": "./bin/setup_prod.sh && react-native run-ios --scheme app --simulator \"iPhone 12\"",
		"test": "jest --detectOpenHandles",
		"test:watch": "jest --watch-all --detectOpenHandles",
		"lint": "eslint \"src/**/*.tsx\" \"src/**/*.ts\"",
		"format": "yarn prettier --write \"src/**/*.tsx\" \"src/**/*.ts\"",
		"clean:ios": "cd ios && bundle exec fastlane clean",
		"build:ios": "./bin/setup_stage.sh && cd ios && bundle exec fastlane build",
		"patch:ios": "./bin/setup_stage.sh && cd ios && bundle exec fastlane patch",
		"minor:ios": "./bin/setup_stage.sh && cd ios && bundle exec fastlane minor",
		"major:ios": "./bin/setup_stage.sh && cd ios && bundle exec fastlane major",
		"release:ios": "./bin/setup_prod.sh && cd ios && bundle exec fastlane release"
	},
	"dependencies": {
		"@notifee/react-native": "^4.0.1",
		"@react-native-async-storage/async-storage": "~1.15.0",
		"@react-native-firebase/app": "14.3.0",
		"@react-native-firebase/auth": "14.3.0",
		"@react-native-firebase/crashlytics": "14.3.0",
		"@react-native-firebase/dynamic-links": "14.3.0",
		"@react-native-firebase/firestore": "14.3.0",
		"@react-native-firebase/functions": "14.3.0",
		"@react-native-firebase/messaging": "14.3.0",
		"@react-native-firebase/storage": "14.3.0",
		"@react-native-menu/menu": "^0.5.1",
		"@react-native-picker/picker": "^2.2.1",
		"@react-navigation/bottom-tabs": "^6.0.9",
		"@react-navigation/native": "^6.0.6",
		"@react-navigation/native-stack": "^6.2.4",
		"@stripe/stripe-react-native": "0.3.0",
		"@types/jest": "^27.0.3",
		"@types/uuid": "^8.3.3",
		"eslint": "^8.0.1",
		"eslint-config-google": "^0.14.0",
		"eslint-plugin-react": "^7.26.1",
		"expo": "^42.0.0",
		"expo-image-picker": "~10.2.2",
		"expo-splash-screen": "~0.11.2",
		"expo-updates": "~0.8.2",
		"pre-commit": "^1.2.2",
		"prettier": "^2.5.1",
		"react": "^17.0.2",
		"react-dom": "16.13.1",
		"react-native": "0.63.4",
		"react-native-fast-image": "^8.5.11",
		"react-native-gesture-handler": "~1.10.2",
		"react-native-get-random-values": "^1.7.0",
		"react-native-image-viewing": "^0.2.1",
		"react-native-keyboard-aware-scroll-view": "^0.9.5",
		"react-native-picker-select": "^8.0.4",
		"react-native-progress": "^5.0.0",
		"react-native-reanimated": "~2.2.0",
		"react-native-safe-area-context": "3.2.0",
		"react-native-screens": "~3.4.0",
		"react-native-snap-carousel": "^3.9.1",
		"react-native-svg": "^12.1.1",
		"react-native-toast-message": "^2.0.2",
		"react-native-unimodules": "^0.14.8",
		"react-native-vector-icons": "^9.0.0",
		"react-native-web": "~0.13.12",
		"remove": "^0.1.5",
		"tailwind-react-native-classnames": "^1.5.1",
		"tslib": "^2.3.1",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@babel/core": "~7.9.0",
		"@prettier/plugin-ruby": "^2.0.0",
		"@testing-library/react-native": "^8.0.0",
		"@types/react": "^17.0.37",
		"@types/react-native": "~0.63.2",
		"@types/react-native-snap-carousel": "^3.8.5",
		"@types/react-native-vector-icons": "^6.4.10",
		"@typescript-eslint/eslint-plugin": "^5.11.0",
		"@typescript-eslint/parser": "^5.11.0",
		"babel-preset-expo": "8.3.0",
		"identity-obj-proxy": "^3.0.0",
		"jest-expo": "^42.0.0",
		"prettier-plugin-organize-imports": "^2.3.4",
		"ts-jest": "^27.0.7",
		"typescript": "~4.0.0"
	},
	"jest": {
		"extensionsToTreatAsEsm": [
			".ts"
		]
	},
	"pre-commit": [
		"lint",
		"format",
		"test"
	],
	"prettier": {
		"trailingComma": "es5",
		"tabWidth": 2,
		"useTabs": true,
		"semi": true,
		"singleQuote": true
	}
}
