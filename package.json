{
	"main": "index.js",
	"name": "deerx",
	"scripts": {
		"dev:ios": "./bin/setup_dev.sh && react-native run-ios --scheme app --simulator",
		"stage:ios": "./bin/setup_stage.sh && react-native run-ios --scheme app --simulator",
		"prod:ios": "./bin/setup_prod.sh && react-native run-ios --scheme app --simulator",
		"dev:android": "./bin/setup_dev.sh && react-native run-android",
		"stage:android": "./bin/setup_stage.sh && react-native run-android",
		"prod:android": "./bin/setup_prod.sh && react-native run-android --variant=release",
		"debug:android": "export JAVA_HOME=`/usr/libexec/java_home -v 11.0.10` && ./bin/setup_dev.sh && react-native run-android",
		"test": "jest --detectOpenHandles",
		"test:watch": "jest --watch-all --detectOpenHandles",
		"lint": "eslint \"src/**/*.tsx\" \"src/**/*.ts\"",
		"format": "yarn prettier --write \"src/**/*.tsx\" \"src/**/*.ts\"",
		"setup:dev": "./bin/setup_dev.sh",
		"setup:stage": "./bin/setup_stage.sh",
		"setup:prod": "./bin/setup_prod.sh",
		"clean:ios": "cd ios && bundle exec fastlane clean",
		"clean:android": "cd android && ./gradlew clean",
		"build:ios": "cd ios && bundle exec fastlane build",
		"build:android": "cd android && ./gradlew bundleRelease",
		"patch:ios": "cd ios && bundle exec fastlane patch",
		"minor:ios": "cd ios && bundle exec fastlane minor",
		"major:ios": "cd ios && bundle exec fastlane major",
		"packager:android": "./node_modules/react-native/scripts/launchPackager.command"
	},
