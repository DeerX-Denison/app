{
	"main": "index.js",
	"name": "deerx",
	"private": true,
	"scripts": {
		"install": "cd ios && (bundle install & pod install --repo-update)",
		"dev:ios:ip12": "./bin/setup_dev.sh && react-native run-ios --scheme app --simulator \"iPhone 12\"",
		"dev:ios:ipSE": "./bin/setup_dev.sh && react-native run-ios --scheme app --simulator \"iPhone SE (3rd generation)\"",
		"stage:ios:ip12": "./bin/setup_stage.sh && react-native run-ios --scheme app --simulator \"iPhone 12\"",
		"stage:ios:ipSE": "./bin/setup_stage.sh && react-native run-ios --scheme app --simulator \"iPhone SE (3rd generation)\"",
		"prod:ios:ip12": "./bin/setup_prod.sh && react-native run-ios --scheme app --simulator \"iPhone 12\"",
		"prod:ios:ipSE": "./bin/setup_prod.sh && react-native run-ios --scheme app --simulator \"iPhone SE (3rd generation)\"",
		"dev:android": "./bin/setup_dev.sh && react-native run-android",
		"debug:android": "export JAVA_HOME=`/usr/libexec/java_home -v 11.0.10` && ./bin/setup_dev.sh && react-native run-android",
		"test": "jest --detectOpenHandles",
		"test:watch": "jest --watch-all --detectOpenHandles",
		"lint": "eslint \"src/**/*.tsx\" \"src/**/*.ts\"",
		"format": "yarn prettier --write \"src/**/*.tsx\" \"src/**/*.ts\"",
		"setup:dev": "./bin/setup_dev.sh",
		"setup:stage": "./bin/setup_stage.sh",
		"setup:prod": "./bin/setup_prod.sh",
		"clean:ios": "cd ios && bundle exec fastlane clean",
		"build:ios": "cd ios && bundle exec fastlane build",
		"patch:ios": "cd ios && bundle exec fastlane patch",
		"minor:ios": "cd ios && bundle exec fastlane minor",
		"major:ios": "cd ios && bundle exec fastlane major"
	},
